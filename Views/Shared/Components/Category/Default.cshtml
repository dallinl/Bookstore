@{ 
}

@model IEnumerable<string>

<div class="d-grip gap-2">
    <a asp-controller="Home" asp-action="Index" asp-route-categoryType="" asp-route-pageNum="1" class="btn btn-outline-dark">Home</a>
    <br />
    @foreach (string x in Model)
    {
        <a asp-controller="Home" asp-action="Index" asp-route-categoryType="@x" asp-route-pageNum="1"
           class="btn @(ViewBag.SelectedType == x ? "btn-primary" : "btn-outline-dark")">@x </a>
        <br />
    }
</div>