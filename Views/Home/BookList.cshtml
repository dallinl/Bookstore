@{
    ViewData["Title"] = "List of Books";
}

@model ProjectsViewModel

<h2> Welcome to the Book Store </h2>
<h5> </h5>

@foreach (var x in Model.Books)
{
    <h4>@x.Title</h4>
    <li>Author: @x.Author</li>
    <li>Publisher: @x.Publisher</li>
    <li>ISBN: @x.Isbn</li>
    <li>Classification: @x.Classification</li>
    <li>Category: @x.Category</li>
    <li>PageCount: @x.PageCount</li>
    <li>Price: $@x.Price</li>
    <br />

}

<div page-blah="@Model.PageInfo" page-action="BookList" page-class="btn" page-classes-enabled=true
     page-class-normal="btn-outline-dark" page-class-selected="btn-primary"></div>


